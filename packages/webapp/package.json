    "@types/jasmine": "~4.3.4",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "karma-spec-reporter": "^0.0.36",
    "karma-verbose-reporter": "^0.0.8",
    "postcss": "^8.4.24",
