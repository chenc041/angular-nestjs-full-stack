{
  "name": "webapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test  --no-watch --browsers=ChromeHeadless",
    "test:coverage": "ng test --code-coverage --browsers=ChromeHeadless",
